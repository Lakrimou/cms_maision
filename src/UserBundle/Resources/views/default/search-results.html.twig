{% if qouiquis is not empty %}
    {% for qq in qouiquis %}
        {% set ville_details %}{% for qville in qq.qvilles %}{{ qville.ville.libelle }}{% endfor %}{% endset %}
        {#{{ dump(qq) }}#}
        <div class="col-md-4 col-sm-6 lp-grid-box-contianer grid_view2 card1 lp-grid-box-contianer1" data-title="AleksandraD Fashion and Design" data-postid="{{ loop.index }}" data-lattitue="36.890203" data-longitute="10.313564" data-posturl="http://studio.listingprowp.com/listing/aleksandrad-fashion-and-design/">
            <div class="lp-grid-box">
                <div class="lp-grid-box-thumb-container">
                    <div class="lp-grid-box-thumb">
                        <div class="show-img">
                            {% set path =  asset('uploads/image/')~qq.user.id ~'/'~ qq.id ~'/avatar/avatar.jpg' %}
                            {% if asset_exists(path) %}
                                <span class="pre_imagecenter"></span>
                                <img src="{{ asset('uploads/image/') }}{{ qq.user.id }}/{{ qq.id }}/avatar/avatar.jpg">
                            {% else %}
                                <img src="{{ asset('tpl/images/ween_no_img.png') }}"/>
                            {% endif %}
                            {#</a>#}
                        </div>
                        <div class="hide-img listingpro-list-thumb">
                            {% set path =  asset('uploads/image/')~qq.user.id ~'/'~ qq.id ~'/avatar/avatar.jpg' %}
                            {% if asset_exists(path) %}
                                <span class="pre_imagecenter"></span>
                                <img src="{{ asset('uploads/image/') }}{{ qq.user.id }}/{{ qq.id }}/avatar/avatar.jpg">
                            {% else %}
                                <img src="{{ asset('tpl/images/ween_no_img.png') }}"/>
                            {% endif %}
                            {#</a>#}
                        </div>
                    </div>
                    <div class="lp-grid-box-quick">
                        <ul class="lp-post-quick-links">
                            <li>
                                <a href="#" data-post-type="grids" data-post-id="{{ loop.index }}" data-success-text="Saved" class="status-btn add-to-fav lp-add-to-fav">
                                    <i class="fa fa-bookmark-o"></i> <span>Enregistrer</span>
                                </a>
                            </li>
                            <li>
                                <a class="icon-quick-eye md-trigger qickpopup" data-modal="modal-{{ loop.index }}"><i class="fa fa-eye"></i> Aperçu</a>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="lp-grid-desc-container lp-border clearfix">
                    <div class="lp-grid-box-description ">
                        <div class="lp-grid-box-left pull-left">
                            <h4 class="lp-h4">
                                <a href="http://{{ qq.slug }}.{{ app.request.server.get("SERVER_NAME") }}">{{ qq.firstName }}</a>
                            </h4>
                            <ul>
                                <li>
                                    <span class="rate lp-rate-satisfactory">2.0<sup>/ 5</sup></span> <span>1 Évaluation</span>
                                </li>

                                <li>
                                    <span class="cat-icon"><i class="fa fa-tag" aria-hidden="true"></i></span>
                                    <a href="javascript:;">
                                        {% for ngn in qq.ngns %}
                                            {{ ngn.category.libelle|upper }}
                                        {% endfor %}
                                    </a>
                                </li>
                            </ul>
                            <div class="review">
                                <div class="review-post">
                                    <div class="reviewer-thumb">
                                        <img src="http://0.gravatar.com/avatar/ceb3b0ac8505f53c921d685e6b7039a2?s=94&#038;d=mm&#038;r=g" alt="">
                                    </div>
                                    <div class="reviewer-details">
                                        <p>{{ qq.details|raw|slice(3,75) }}</p>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="lp-grid-box-right pull-right">
                        </div>
                    </div>
                    <div class="lp-grid-box-bottom">
                        <div class="pull-left">
                            <div class="show">
                                <span class="cat-icon">
                                    <img class="icon icons8-mapMarkerGrey"
                                         src="data:image/png;base64,"
                                         alt="mapMarkerGrey">
                                </span>
                                <a href="javascript:;">
                                    {{ ville_details }}
                                </a>
                            </div>
                            <div class="hide">
                                                    <span class="cat-icon">
                                                        <img class="icon icons8-mapMarkerGrey"
                                                             src="data:image/png;base64,"
                                                             alt="mapMarkerGrey">
                                                    </span>
                                <span class="text gaddress">{{ qq.address1 }} - {{ qq.zipCode }}</span>
                            </div>
                        </div>

                        {#<div class="pull-right">#}
                            {#<a href="#" class="status-btn"><span class="grid-closed">Closed Now!</span>#}
                            {#</a>#}
                        {#</div>#}
                        <div class="clearfix"></div>
                    </div>
                </div>
            </div>
        </div>

        {#========================== MODAL ====================#}

        <div class="md-modal md-effect-3" id="modal-{{ loop.index }}">
            <div class="container">
                <div class="md-content ">
                    <div class="row popup-inner-left-padding ">
                        <div class="col-md-6 lp-insert-data">
                            <div class="lp-grid-box-thumb">

                                <div class="show-img-preview">
                                    {% set path =  asset('uploads/image/')~qq.user.id ~'/'~ qq.id ~'/avatar/avatar.jpg' %}
                                    {% if asset_exists(path) %}
                                        <img src="{{ asset('uploads/image/') }}{{ qq.user.id }}/{{ qq.id }}/avatar/avatar.jpg">
                                    {% else %}
                                        <img src="{{ asset('tpl/images/ween_no_img.png') }}"/>
                                    {% endif %}
                                </div>
                            </div><!-- ../grid-box-thumb -->
                            <div class="lp-grid-desc-container lp-border clearfix">
                                <div class="lp-grid-box-description ">
                                    <div class="lp-grid-box-left pull-left">
                                        <h4 class="lp-h4">
                                            <a href="http://{{ qq.slug }}.{{ app.request.server.get("SERVER_NAME") }}">
                                                {{ qq.firstName }}
                                            </a>
                                        </h4>
                                        <ul>
                                            <li>
                                                <span class="rate">2.0<sup>/5</sup></span>
                                                1 Reviews
                                            </li>
                                            <li>
                                              <span class="cat-icon">
                                                  <i class="fa fa-tag" aria-hidden="true"></i>
                                              </span>
                                                    <a href="javascript:;">
                                                        {% for ngn in qq.ngns %}
                                                            {{ ngn.category.libelle|upper }}
                                                        {% endfor %}
                                                    </a>
                                            </li>
                                            <li>
                                                <a href="tel:(415) 312-2458">
                                                    {{ qq.phone }} </a>
                                            </li>
                                        </ul>
                                        <div class="lp-grid-desc">
                                            <p>{{ qq.details|raw|slice(3,160) }}...<a href="http://{{ qq.slug }}.{{ app.request.server.get("SERVER_NAME") }}">Plus de details</a></p>
                                        </div>
                                    </div>
                                    <div class="lp-grid-box-right pull-right">
                                    </div>
                                </div>
                                <div class="lp-grid-box-bottom">
                                    <div class="pull-left">
																			<span class="cat-icon">
											<img class="icon icons8-Food"
                                                 src="data:image/png;base64,"
                                                 alt="cat-icon">
										</span>
                                        <span class="text gaddress">{{ qq.address1 }} - {{ qq.zipCode }}</span>
                                    </div>

                                    {#<div class="pull-right">#}
                                        {#<a href="#" class="status-btn"><span class="grid-closed">Closed Now!</span>#}
                                        {#</a>#}
                                    {#</div>#}
                                    <div class="clearfix"></div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div id="quickmap{{ loop.index }}" class="quickmap"></div>
                            <a class="md-close widget-map-click"><i class="fa fa-close"></i></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    {% endfor %}
{% else %}
    <div class="col-md-12 ">
        pas de resultat
    </div>
    {% block stylesheet %}
        <style>
            div#mm-0 {
                background: url({{ asset('tpl/images/bgWeen.png') }});
                height: 100%;
            }
        </style>
    {% endblock %}
{% endif %}
