    {% if events %}
        <div id="events-section">
            <div class="container">
                <div class="row">
                    <div class="col-md-12 text-center">
                        <div id="easy-search-events">
                            <h1><span>Quick Events</span></h1>
                        </div>
                        {% for ev in events %}
                            <div class="col-sm-6">
                                <div class="listing-boxes-container">
                                    <div class="col-sm-12 col-md-4 nopadding">
                                        {% if ev.image %}
                                        <div class="listing-boxes-image">
                                            <img src="{{ asset('uploads/events/'~ ev.image ~'.jpg')}}" alt="events" />
                                            {% if ev.eventName %}
                                            <h1><span>{{ ev.eventName }}</span></h1>
                                            {% endif %}
                                        </div>
                                        {% else %}
                                        <div class="listing-boxes-image">
                                            <img src="{{ asset('uploads/events/Default/quoiqui-events.jpg') }}" alt="events-defaults" }} alt="listing 1">
                                            {% if ev.eventName %}
                                            <h1><span>{{ ev.eventName }}</span></h1>
                                            {% endif %}
                                        </div>
                                        {% endif %}
                                    </div>
                                    <div class="col-sm-12 col-md-8 nopadding">
                                        <div class="listing-boxes-text">
                                            {#<h2>#}
               {#<span>#}
               {#<i class="fa fa-star"></i>#}
               {#<i class="fa fa-star"></i>#}
               {#<i class="fa fa-star"></i>#}
               {#<i class="fa fa-star-half-empty"></i>#}
               {#4.5#}
               {#</span>#}
                                            {#</h2>#}
                                            {% if ev.qville %}
                                            <a href="#">
                                                <h3>{{ ev.qville.qouiqui.firstName }} {{ ev.qville.qouiqui.lastName }}</h3>
                                            </a>
                                            {% endif %}
                                            {% if ev.details %}
                                            <p>{{ eventLength(ev.details)}}</p>
                                            {% endif %}
                                            {% if ev.qville %}
                                            <h5>{{ ev.qville.ville.libelle }}</h5>
                                            {% endif %}
                                            {% if  ev.dateStart|date("m/d/Y") and ev.dateEnd|date("m/d/Y") %}
                                            <span class="glyphicon glyphicon-calendar"><a href="#"> {{ ev.dateStart|date("m/d/Y") }} | {{ ev.dateEnd|date("m/d/Y") }}</a></span>
                                            {% endif %}
                                            <span> <a href="{{ path('dashboard_booking_reservation', { 'id': ev.id }) }}" class="btn btn-warning">Reservées</a></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        {% endfor %}
                    </div>
                </div>
            </div>
        </div>
    {% endif %}

