<section id="section-annonces" class="property-details padding">
    <div class="container">
        <div class="col-md-12">
        {% if app.user %}
            {% if app.user.id==quoiqui.user.id %}
                <div class="row">
                    <div class="col-md-10 col-sm-4"></div>
                    <div class="col-md-2 col-sm-3">
                        <button type="button" class="button11" id="btn_submit" name="btn_submit" data-toggle="modal" data-target="#listVoiture">Gérer cette section</button>
                    </div>
                </div>
            {% endif %}
        {% endif %}
        <div class="row">
            <div class="col-xs-12 bottom40">
                <h2 class="text-uppercase">NOS <span class="color_red">VOITURES DE LOCATION</span></h2>
                <div class="line_1"></div>
                <div class="line_2"></div>
                <div class="line_3"></div>
            </div>
        </div>
        <div class="col-md-2">
            <div class="col-md-2"><p>Recherchez une location de voiture</p></div>
            <form id="add_vehicule" action="{{ path('location_voiture_add_vehicule') }}" method="post" enctype="multipart/form-data">
                <input type="hidden" name="qouiqui_id" value="{{ quoiqui.id }}">
                        <div class="form-group col-md-12">
                            <label>Série :</label>
                            <input type="text" class="form-control" id="serie" name="serie" placeholder="Exemples: 351,180,456...">
                        </div>
            </form>
        </div>
        <div class="col-md-10">
            {% if locationvoitures|length==2 %}
                <div class="col-md-2 col-sm-4 col-xs-12"></div>
            {% endif %}
            {% if locationvoitures|length==1 %}
                <div class="col-md-4 col-sm-4 col-xs-12"></div>
            {% endif %}
            {% if locationvoitures|length ==0 %}
                <div class="col-md-4 col-sm-4 col-xs-12"></div>
                <div class="col-md-5 col-sm-4 top40">
                    <h1>Aucune voiture disponible</h1>
                </div>
            {% else %}
                <div id="list-par-neuf" class="row">
                    <div class="row"><div class="col-md-9"><h1>Les modéles des voitures disponibles :</h1></div></div>
                    <div id="property-2-slider" class="owl-carousel">
                            <div class="item">
                                <div class="property_item bottom40">
                                    <div class="image">
                                        <img src="https://d2yb1bs45difkc.cloudfront.net/320x180/phpsgm7RR_584ffda06b567.jpg" alt="" class="img-responsive" style="height: 100%; width: 100%;">
                                        <div class="price">
                                                            <span class="tag">
                                                                   RENAULT SYMBOL
                                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="item">
                            <div class="property_item bottom40">
                                <div class="image">
                                    <img src="https://d2yb1bs45difkc.cloudfront.net/320x180/php9p2mxQ_583d5cee61177.jpg" alt="" class="img-responsive" style="height: 100%; width: 100%;">
                                    <div class="price">
                                                            <span class="tag">
                                                                   FIAT PUNTO
                                                            </span>
                                    </div>
                                </div>
                            </div>
                            </div>
                                <div class="item">
                                <div class="property_item bottom40">
                                    <div class="image">
                                        <img src="https://d2yb1bs45difkc.cloudfront.net/320x180/phpK9CGRX_58233975a02d0.jpg" alt="" class="img-responsive" style="height: 100%; width: 100%;">
                                        <div class="price">
                                                            <span class="tag">
                                                                  FORD FIESTA
                                                            </span>
                                        </div>
                                    </div>
                                </div>
                        </div>
                        <div class="item">
                            <div class="property_item bottom40">
                                <div class="image">
                                    <img src="https://d2yb1bs45difkc.cloudfront.net/320x180/phpGkwvG0_5881e1b7f2dcd.jpg" alt="" class="img-responsive" style="height: 100%; width: 100%;">
                                    <div class="price">
                                                            <span class="tag">
                                                                  HYUNDAI I20
                                                            </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    {% for locationvoiture in locationvoitures|reverse %}
                        <div class="item col-md-4">
                            <div class="property_item bottom40">
                                <div class="image">

                                    {% if asset_exists('/uploads/image/'~quoiqui.user.id~'/'~quoiqui.id~'/vehicule/'~locationvoiture.id~'.png') %}
                                        {% set img =  '/uploads/image/'~quoiqui.user.id~'/'~quoiqui.id~'/vehicule/'~locationvoiture.id~'.png' %}
                                    {% else %}
                                        {% set img = '/uploads/image/'~quoiqui.user.id~'/'~quoiqui.id~'/vehicule/car.png' %}
                                    {% endif %}

                                    <img src="{{ img }}" alt="listin" class="img-responsive" style="max-height: 125px;">

                                    <div class="property_meta">
                                        <span><i class="fa fa-user-circle"></i>{{ locationvoiture.nbrplace }} Places</span>
                                        <span><i class="fa fa-money"></i>{{ locationvoiture.prixJour }} DT</span>
                                    </div>
                                    <div class="price">
                                    <span class="tag">
                                {% if locationvoiture.statut == "reserves" %}
                                    Non disponible
                                {% endif %}
                                        {% if locationvoiture.statut == "non_reserves" %}
                                            Disponible dès maintenant
                                        {% endif %}
                                    </span>
                                    </div>
                                    <div class="overlay">
                                        <div class="centered"><a class="link_arrow white_border" data-toggle="modal" data-target="#viewDetail{{ locationvoiture.id }}">Voir les détails</a></div>
                                    </div>
                                </div>
                                <div class="proerty_content">
                                    <div class="proerty_text" style="height: 150px;">
                                        <h3>Serie: {{ locationvoiture.serie }}&nbsp;</h3>
                                        {% if(locationvoiture.model) %}
                                            <span class="bottom10">Marque: {{ locationvoiture.model.mark.name }}&nbsp;</span>
                                            <p>Modele: {{ locationvoiture.model.name }}</p>
                                        {% endif %}
                                    </div>
                                    <div class="favroute clearfix">
                                        <ul class="pull-left" style="margin-left: 5px;">
                                            <li><i class=""></i>
                                                {% if locationvoiture.statut == "reserves" %}
                                                    Non disponible
                                                {% endif %}
                                                {% if locationvoiture.statut == "non_reserves" %}
                                                    Disponible dès maintenant
                                                {% endif %}
                                            </li>
                                        </ul>
                                        <p class="pull-right" style="margin-right: 5px;"><i class="fa fa-money"></i> {{ locationvoiture.prixJour }} DT par jour</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    {% endfor %}
                </div>

                {% if(page>1) %}
                    <div class="col-md-4"></div>
                    <button class="button11 col-md-4" id="afficherPlus" count="{{ count }}" debut="{{ (page-1)*9 }}" page="{{ page-1 }}">Afficher plus</button>
                {% endif %}
            {% endif %}
        </div>
    </div>
</section>
{# modal liste des voitures#}
<div class="modal fade" id="listVoiture" tabindex="-1" role="dialog" aria-labelledby="listeVoiture" aria-hidden="true">
    <div class="modal-dialog" role="document" style="    width: 90%;">
        <div class="modal-content">
            <div class="modal-header">
                <div class="col-md-10 col-sm-4">
                    <div class="col-xs-12 bottom40">
                        <h2 class="text-uppercase">Nos <span class="color_red">annonces</span></h2>
                    </div>
                </div>
                <div class="col-md-2 col-sm-3">
                    <button type="button" class="button11" data-toggle="modal" data-target="#addVoiture" id="add">Nouvelle annonce</button>
                </div>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <table id="" class="dataTable" cellspacing="0" width="100%">
                    <thead>
                    <tr>
                        <th>Numéro</th>
                        <th>serie</th>
                        <th>Nombre de Places</th>
                        <th>prix de Jour</th>
                        <th>statut</th>
                        <th>marque</th>
                        <th>model</th>

                        <th>Modification</th>
                        <th>Suppression</th>
                    </tr>
                    </thead>
                    <tfoot>
                    <tr>
                        <th>Numéro</th>
                        <th>serie</th>
                        <th>Nombre de Places</th>
                        <th>prix de Jour</th>
                        <th>statut</th>
                        <th>marque</th>
                        <th>model</th>

                        <th>Modification</th>
                        <th>Suppression</th>
                    </tr>
                    </tfoot>
                    <tbody>
                    {% for locationvoiture in locationvoitures|reverse %}
                        <tr data-tr-voiture="{{ locationvoiture.id }}">
                            <td>{{ locationvoiture.id }}</td>
                            <td>{{ locationvoiture.serie }}</td>
                            <td>{{ locationvoiture.nbrPlace }}</td>
                            <td>{{ locationvoiture.prixJour }}</td>
                            <td>
                                {% if locationvoiture.statut == "reserves" %}
                                    Non disponible
                                {% endif %}
                                {% if locationvoiture.statut == "non_reserves" %}
                                    Disponible dès maintenant
                                {% endif %}
                            </td>
                            <td>
                                {% if locationvoiture.model %}
                                    {{ locationvoiture.model.mark.name }}
                                {% endif %}
                            </td>
                            <td>
                                {% if locationvoiture.model %}
                                    {{ locationvoiture.model.name }}
                                {% endif %}
                            </td>

                            <td>
                                <button type="button" class="btn btn-info btn-lg edit" data-link-edit="{{ path('location_voiture_id',{'id': locationvoiture.id}) }}"><i class="fa fa-pencil-square" aria-hidden="true"></i></button>
                            </td>
                            <td>
                                <button class="btn btn-danger deleteAuto" data-id="{{ locationvoiture.id }}" data-link="{{ path('location_voiture_delete') }}"><i class="fa fa-trash-o" aria-hidden="true"></i></button>
                            </td>
                        </tr>
                    {% endfor %}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
{#modal add  voiture#}
<div class="modal fade" id="addVoiture" role="dialog" aria-labelledby="ajoutvoiture" aria-hidden="true">
    <div class="modal-dialog" role="document" style="width: 100%">
        <div class="modal-content">
            <div class="modal-header">
                <h1>Une nouvelle véhicule</h1>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form id="add_vehicule" action="{{ path('location_voiture_add_vehicule') }}" method="post" enctype="multipart/form-data">
                    <input type="hidden" name="qouiqui_id" value="{{ quoiqui.id }}">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group col-md-9">
                                <label>Photo de l'image :</label>
                                <input type="file" class="form-control" name="img" alt="">
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group col-md-4">
                                <label>Série :</label>
                                <input type="text" class="form-control" id="serie" name="serie" placeholder="Exemples: 351,180,456...">
                            </div>
                            <div class="form-group col-md-4">
                                <label>Mark :</label>
                                <select id="marque" name="mark" class="form-control" style="width: 100%">
                                    <option selected value="">Choisissez votre marque</option>
                                </select>
                            </div>
                            <div class="form-group col-md-4">
                                <label>Modele :</label>
                                <select id="modeles" name="modele" class="form-control" style="width: 100%">
                                    <option selected value="">Choisissez votre modéle</option>
                                </select>
                            </div>
                            <div class="form-group col-md-8"></div>
                            <div class="form-group col-md-4">
                                <label>Nombre de places :</label>
                                <input type="number" class="form-control" id="nbr_place" name="nbr_place" placeholder="Exemples: 1,2,3...">
                            </div>
                            <div class="form-group col-md-4">
                                <label>Prix par jour :</label>
                                <input type="number" class="form-control" id="prix" name="prix" step="0.01" placeholder="Exemples:154,25 DT ">
                            </div>
                            <div class="form-group col-md-4">
                                <label>Statut :</label><br/>
                                <label><input type="radio" name="statut" value="non_reserves" checked> Disponible </label><br/>
                                <label><input type="radio" name="statut" value="reserves"> Non disponible </label>
                            </div>
                            <div class="form-group col-md-9"></div>
                            <div class="form-group col-md-4">
                                <label>Ports :</label>
                                <select id="ports" name="ports" class="form-control" style="width: 100%">
                                    <option selected value="2">2</option>
                                    <option value="4">4</option>
                                </select>
                            </div>
                            <div class="form-group col-md-4">
                                <label>Energie :</label>
                                <select class="selectpicker" id="energy" name="energy" style="width: 100%">
                                    <option value="diesel">Diesel</option>
                                    <option value="essence">Essence</option>

                                </select>
                            </div>
                            <div class="form-group col-md-4">
                                <label>Année-modèle:</label>
                                <input type="number" class="form-control" id="model_year" name="model_year" placeholder="exp:2015*">
                            </div>
                            <div class="form-group col-md-9"></div>
                            <div class="form-group col-md-4">
                                <label>Energie en KW</label>
                                <input type="text" class="form-control" id="kiloW" name="kiloW" placeholder="exp:235*">
                            </div>
                            <div class="form-group col-md-4">
                                <label>Energie fiscale:</label>
                                <input type="number" class="form-control" id="fiscal_power" name="fiscal_power" placeholder="exp:6*">
                            </div>
                            <div class="form-group col-md-4">
                                <label>Kilométrage:</label>
                                <input type="number" class="form-control" id="mileage" name="mileage" placeholder="exp:40000*">
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group col-md-4">
                                <label>Petits Valises:</label>
                                <select class="selectpicker" id="petitValises" name="petitValises" style="width: 100%">
                                    <option value="1">1</option>
                                    <option value="2">2</option>
                                </select>
                            </div>
                            <div class="form-group col-md-4">
                                <label>Grand valises:</label>
                                <select class="selectpicker" id="grandvalises" name="grandvalises" style="width: 100%">
                                    <option value="1">1</option>
                                    <option value="2">2</option>
                                </select>
                            </div>
                            <div class="form-group col-md-4">
                                <label>Boite:</label>
                                <select class="selectpicker" id="boite" name="boite" style="width: 100%;">
                                    <option value="mecanique" selected>Mécanique</option>
                                    <option value="automatique">Automatique</option>
                                </select>
                            </div>
                            <div class="form-group col-md-9"></div>
                            <div class="form-group col-md-4">
                                <label>Avec Conducteur:</label>
                                <select class="selectpicker" id="conducteur" name="conducteur" style="width: 100%;">
                                    <option value="1" selected>Oui</option>
                                    <option value="0">Non</option>
                                </select>
                            </div>
                            <div class="form-group col-md-4">
                                <label>Avec GPS:</label>
                                <select class="selectpicker" id="gps" name="gps" style="width: 100%;">
                                    <option value="1" selected>Oui</option>
                                    <option value="0">Non</option>
                                </select>
                            </div>
                            <div class="form-group col-md-4">
                                <label>Avec air:</label>
                                <select class="selectpicker" id="aire" name="aire" style="width: 100%;">
                                    <option value="1" selected>Oui</option>
                                    <option value="0">Non</option>
                                </select>
                            </div>
                            <div class="form-group col-md-9"></div>
                            <div class="form-group col-md-8">
                                <label>Sieges independant arriere:</label>
                                <select class="selectpicker" id="sieges" name="sieges" style="width: 100%;">
                                    <option value="1" selected>Oui</option>
                                    <option value="0">Non</option>
                                </select>
                            </div>
                            <div class="form-group col-md-12">
                                <label>Description:</label>
                                <textarea class="form-control" id="description" name="description" style="width: 100%;">

                                </textarea>
                            </div>
                            <div class="form-group col-md-9"></div>
                            <div class="form-group col-md-8"></div>
                            <div class="form-group col-md-4">
                                <button type="reset" class="button11" data-dismiss="modal">Anuler</button>
                                <button type="submit" class="button11" id="btn_add_vehicule" name="addVoiture" data-target="modifier" data-selector="add_vehicule">Enregistrer</button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="row"></div>
            <div class="modal-footer">
                <button type="reset" class="service-details-btn btn-default btn-xs pull-right suite" data-dismiss="modal">Fermer</button>
            </div>
        </div>
    </div>
</div>
{#modal Edit  voiture#}
