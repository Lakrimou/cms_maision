<section id="section-services" class="we_are bg_light padding">
    {% if attributes.connected %}

            <div class="pull-righ" style="    margin-right: 24px;">
                <div data-step="3" data-intro="This is a tooltip!" class="pull-right margin-bott-40">
                    <button type="button" class="theme-btn button11 btn-style-one pull-right" data-toggle="modal"
                            data-target="#service-modal">Gérer cette section
                    </button>
                </div>
            </div>

    {% endif %}
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h2 class="text-uppercase">Nos <span class="color_red">Formations</span></h2>
                <div class="line_1"></div>
                <div class="line_2"></div>
                <div class="line_3"></div>
            </div>
        </div>
        <div class="row" id="append-item-frm">

            {% if formation is defined %}
                {% set nb = formation|length %}
                {% for  b in formation[:3]|sort|reverse %}
                    {% set image= asset('uploads/image/')~attributes.quoiqui.user.id~"/"~ attributes.quoiqui.id ~"/service/"~ b.idfrm ~'.jpg' %}
                    {% if (formation|length == 1) %}
                        <div class="col-md-4 col-sm-4 col-xs-12"></div>
                    {% endif %}
                    {% if (formation|length == 2) and (loop.index == 1) %}
                        <div class="col-md-2 col-sm-2 col-xs-12"></div>
                    {% endif %}

                    <a href="#" data-toggle="modal" data-target="#voir_plus" data-idqq="{{ attributes.quoiqui.id }}" class="voir_detail" data-idServ="{{ b.idfrm }}" class="lire_plus">
                        <div class=" col-md-4 col-sm-4 top40 item-id-{{ b.idfrm }}  item-scroll col-custom " style="min-height: 380px;">
                            <div class="block-service margin15 pad15  " style="background: white; ">
                                <div class="row">


                                    <div class="col-md-12 text-center">
                                        {% if   image is defined %}
                                        <div class=" image-content-cf-serv " style="background-image: Url('{{ asset('uploads/image/') }}{{ attributes.quoiqui.user.id }}/{{ attributes.quoiqui.id }}/service/{{ b.idfrm ~'.jpg' }}')"></div>
                                        {% elseif image is not defined %}
                                        <span class="icon-shortcode">
                                {% if 'fa ' in b["serviceIcon"] %}
                                    <i class="{% if b["serviceIcon"] is defined %}{{ b["serviceIcon"] }}{% endif %}" aria-hidden="true"></i>
                                    {% else %}
                                    <img class="svg" width="80px" src='{{ b["serviceIcon"] }}' alt="">
                                {% endif %}

                                    </span>
                                        {% endif %}
                                    </div>

                                    <h3 class="serviceTitle text-center pad15"> {% if  b.ncour  is defined %}{{ b.ncour }}{% endif %} </h3>
                                    <div class="clearfix">
                                        <div class="separator" style="width: 100px;border-bottom-width: 2px; "></div>
                                    </div>
                                    <div class="margin28">
                                        <ul>
                                            <li>
                                                <i class="fa fa-calendar" aria-hidden="true" style="color: red;"></i><span style="margin-left: 10px;" id="dure"> du {{ b.start_cour|date('d-m-y') }}à {{ b.end_cour|date('d-m-y') }}</span>
                                            </li>
                                            <li>
                                                <i class="fa fa-clock-o" aria-hidden="true" style="color: red;"></i><span style="  margin-left: 14px;" id="dure">{{ b.dure }} Heures</span>
                                            </li>
                                        </ul>
                                    </div>
                                    {% if is_mobile() %}
                                        <div class="row service pad15">
                                            <a href="#" data-toggle="modal" data-target="#voir_plus" data-idqq="{{ attributes.quoiqui.id }}" data-idServ="{{ b.idfrm }}" class="voir_detail btn-default btn-xs pull-right suite">En savoir plus</a></div>
                                    {% endif %}
                                </div>

                                {% if not is_mobile() %}
                                    <div class="row service pad15">
                                        <a href="#" data-toggle="modal" data-target="#voir_plus" data-idqq="{{ attributes.quoiqui.id }}" data-idServ="{{ b.idfrm }}" class="voir_detail btn-default btn-xs pull-right suite">En savoir plus</a>
                                    </div>
                                {% endif %}

                            </div>
                        </div>
                    </a>

                {% endfor %}
            {% endif %}

        </div>


        <div class="pull-left margin-bott-40">
            {% if nb>3 %}
                <span id="load-more-service-frm" class="theme-btn button11 btn-style-one pull-left">Voir plus</span>
            {% endif %}
            <span class="load-less-service theme-btn button11 btn-style-one pull-left">Voir moins</span>
        </div>

    </div>

    {% if not is_mobile() %}
        <div class="paralax-icon">
            <div class="particle elem1 anim-delay1"></div>
            <div class="particle elem2 anim-delay5"></div>
            <div class="particle elem3 anim-delay3"></div>
            <div class="particle elem4 anim-delay8"></div>
            <div class="particle elem5 anim-delay9"></div>
            <div class="particle elem6 anim-delay2"></div>
            <div class="particle elem7 anim-delay4"></div>
            <div class="particle elem8 anim-delay3"></div>
            <div class="particle elem9 anim-delay10"></div>
            <div class="particle elem8 anim-delay11"></div>
            <div class="particle elem6 anim-delay12"></div>
            <div class="particle elem1 anim-delay14"></div>
            <div class="particle elem13 anim-delay16"></div>
            <div class="particle elem17 anim-delay18"></div>
            <div class="particle elem2 anim-delay6"></div>
            <div class="particle elem16 anim-delay9"></div>
            <div class="particle elem8 anim-delay3"></div>
            <div class="particle elem9 anim-delay10"></div>
            <div class="particle elem18 anim-delay11"></div>
            <div class="particle elem11 anim-delay12"></div>
            <div class="particle elem12 anim-delay14"></div>
            <div class="particle elem13 anim-delay16"></div>
            <div class="particle elem14 anim-delay18"></div>
            <div class="particle elem15 anim-delay6"></div>
            <div class="particle elem16 anim-delay9"></div>
            <div class="particle elem17 anim-delay7"></div>
            <div class="particle elem18 anim-delay6"></div>
            <div class="particle elem19 anim-delay3"></div>
        </div>
    {% endif %}
</section>


<div id="service-modal" class=" modal  fade" role="dialog">
    <div class="modal-dialog modal-md ">

        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Liste des formations</h4>
            </div>

            <div class="modal-body ">
                <table class="table dataTable " cellspacing="0" width="100%">
                    <thead>
                    <tr>
                        <th width="74">Image</th>
                        <th>Formation</th>
                        <th>Prix</th>
                        <th>date-début</th>
                        <th>date-fin</th>
                        <th>durée</th>
                        <th>Modifier</th>
                        <th>Supprimer</th>
                    </tr>
                    </thead>
                    <tbody>
                    {% for b in formation|sort|reverse %}

                        <tr data-td="{{ b.idfrm }}">

                            <td align="center">

                                <img src="{{ asset('uploads/image/') }}{{ attributes.quoiqui.user.id }}/{{ attributes.quoiqui.id }}/service/{{ b.idfrm ~'.jpg' }}" data-pin-nopin="true" style="  width:  74px;">

                            </td>
                            <td align="center"> {{ b.ncour }}</td>
                            <td align="center">{{ b.prix }}Dt</td>
                            <td align="center">{{ b.start_cour }}</td>
                            <td align="center">{{ b.end_cour }}</td>
                            <td align="center">{{ b.dure }} Heures</td>
                            <td align="center">
                                <a href="#" class=" btn btn-info a-btn-slide-text modifier" data-serv="{{ b.idfrm }}" data-id="{{ attributes.quoiqui.id }}" data-toggle="modal" data-target="#edit_service">
                                    <span class="glyphicon glyphicon-edit" aria-hidden="true"></span>
                                    <span><strong>Modifier</strong></span>
                                </a>

                            </td>


                            <td align="center">

                                <a href="{{ path('module_centreformation_delate_services',{"idserv":  b.idfrm ,"idqq": attributes.quoiqui.id}) }}" class=" btn btn-danger a-btn-slide-text service_suprim click-off" onclick="clickOffConfirmed = confirm('Vous voulez le  supprimer ?');">
                                    <span class="glyphicon glyphicon-remove " aria-hidden="true"></span>
                                    <span><strong>Supprimer</strong></span>
                                </a>
                            </td>

                        </tr>
                    {% endfor %}
                    </tbody>
                </table>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary btn-ajout" data-toggle="modal" data-target="#modal_service">Ajouter une Nouvelle Formation</button>
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Fermer</button>
                </div>
            </div>

        </div>
    </div>
</div>

<div id="modal_service" class="modal  fade" role="dialog">
    <div class="modal-dialog modal-md ">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Ajouter une Formation</h4>
            </div>
            <div class="modal-body row ">
                <form id="ajout_services_frm" method="POST" action="{{ path('module_centreformation_service') }}" enctype="multipart/form-data">
                    <div class="col-md-12">
                        <div class="col-md-6">
                            <input type="hidden" name="qq_id" value="{{ attributes.quoiqui.id }}">
                            <input type="hidden" name="idfrm" value="{{ uniqueId() }}">
                            <div class=" form-group">
                                <label class="col-md-12">Nom de la Formation*</label>
                                <input type="text" placeholder="Nom de la formation" class=" form-control" name="ncour" required="required">
                            </div>
                            <div class="form-group">
                                <label class="col-md-12">Description</label>
                                <textarea placeholder="Entrer description.." rows="3" class="form-control ckeditor" name="desc" required="required"></textarea>
                            </div>
                            <div class="col-sm-6 form-group">
                                <label class="col-md-12">Date début* </label>
                                <div class="form-group">
                                    <div class="bfh-datepicker" data-name="start_cour" data-min="today" data-format="d-m-y" aria-required="true"></div>
                                </div>
                            </div>
                            <div class="col-sm-6 form-group">
                                <label class="col-md-12">Date fin*</label>
                                <div class="form-group">
                                    <div class="bfh-datepicker" data-name="end_cour" data-format="d-m-y" data-min="today" aria-required="true"></div>
                                </div>
                            </div>
                            <div class="col-sm-6 form-group">
                                <label class="col-md-12">Prix</label>
                                <input type="number" placeholder="Entrer le prix.." class="form-control" name="prix">
                            </div>
                            <div class="col-sm-6 form-group">
                                <label class="col-md-12">Durée* </label>
                                <input type="number" placeholder="Entrer durée de la formation.." class="form-control" name="dure" required="required">
                            </div>
                            <div class="checkbox col-md-12 ">
                                <div class="input-group">
                                     <span class="input-group-addon beautiful">
                                        <input type="checkbox" name="certif">
                                       </span>
                                    <input type="text" class="form-control " value="Formation certifiée">
                                    {#<span class="input-group-addon">#}
                                    {#<input type="radio" name="certif">#}
                                    {#</span>#}
                                    {#<input type="text" class="form-control" value="Formation certifiée">#}
                                </div>

                            </div>

                            <div class="col-sm-12 form-group">
                                <label class="col-md-12">Choisissez une photo*</label>
                                <input autocomplete="off" class="form-control " name="file" type="file" required="required" placeholder="choisissez_image" data-items="8"/>
                            </div>
                        </div>


                        <div class="col-md-6 ">
                            <div class="form-group">
                                <label class="col-md-12">Les horaires de la formation</label>
                            </div>
                            <div id="no-more-tables">
                                <table class="table-bordered table-striped table-condensed cf hour-table">
                                    <thead class="cf">
                                    <tr>
                                        <th>Jours</th>
                                        <th>du</th>
                                        <th>à</th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <tr class="day-monday jours" data-open="9:00" data-close="17:00">
                                        <td data-title="Jours">Lundi</td>
                                        <td data-title="du">
                                            <div class='input-group date'>
                                                <input type='text' name="qouiqui[Lun][start]" class="form-control timepick2 "/><span class="input-group-addon"><span class="glyphicon glyphicon-time"></span> </span>
                                            </div>

                                        </td>
