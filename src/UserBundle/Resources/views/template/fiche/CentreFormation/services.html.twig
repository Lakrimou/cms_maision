                                        <td data-title="Jours">Mardi</td>
                                        <td data-title="du">
                                            <div class='input-group date'>
                                                <input type='text' name="qouiqui[Mar][start]" class="form-control  timepick2 time0"/><span class="input-group-addon"><span class="glyphicon glyphicon-time"></span> </span>
                                            </div>

                                        </td>
                                        <td data-title="à">
                                            <div class='input-group date'>
                                                <input type='text' name="qouiqui[Mar][end]" class="form-control  timepick2 time1"/><span class="input-group-addon"><span class="glyphicon glyphicon-time"></span> </span>
                                            </div>

                                        </td>

                                    </tr>

                                    <tr class="day-wednesday" data-open="9:00" data-close="17:00">
                                        <td data-title="Jours">Merecredi</td>
                                        <td data-title="du">
                                            <div class='input-group date'>
                                                <input type='text' name="qouiqui[Mer][start]" class="form-control  timepick2"/><span class="input-group-addon"><span class="glyphicon glyphicon-time"></span> </span>
                                            </div>

                                        </td>
                                        <td data-title="à">
                                            <div class='input-group date'>
                                                <input type='text' name="qouiqui[Mer][end]" class="form-control  timepick2"/><span class="input-group-addon"><span class="glyphicon glyphicon-time"></span> </span>
                                            </div>

                                        </td>

                                    </tr>
                                    <tr class="day-thursday" data-open="9:00" data-close="17:00">
                                        <td data-title="Jours">Jeudi</td>
                                        <td data-title="du">
                                            <div class='input-group date'>
                                                <input type='text' name="qouiqui[Jeu][start]" class="form-control  timepick2"/><span class="input-group-addon"><span class="glyphicon glyphicon-time"></span> </span>
                                            </div>


                                        </td>
                                        <td data-title="à">
                                            <div class='input-group date'>
                                                <input type='text' name="qouiqui[Jeu][end]" class="form-control  timepick2"/><span class="input-group-addon"><span class="glyphicon glyphicon-time"></span> </span>
                                            </div>

                                        </td>

                                    </tr>
                                    <tr class="day-friday" data-open="9:00" data-close="17:00">
                                        <td data-title="Jours">Vendredi</td>
                                        <td data-title="du">
                                            <div class='input-group date'>
                                                <input type='text' name="qouiqui[Ven][start]" class="form-control  timepick2"/><span class="input-group-addon"><span class="glyphicon glyphicon-time"></span> </span>
                                            </div>

                                        </td>
                                        <td data-title="à">
                                            <div class='input-group date'>
                                                <input type='text' name="qouiqui[Ven][end]" class="form-control  timepick2"/><span class="input-group-addon"><span class="glyphicon glyphicon-time"></span> </span>
                                            </div>

                                        </td>

                                    </tr>
                                    <tr class="day-saturday" data-open="9:00" data-close="17:00">
                                        <td data-title="Jours">Samedi</td>
                                        <td data-title="du">
                                            <div class='input-group date'>
                                                <input type='text' name="qouiqui[Sam][start]" class="form-control  timepick2"/><span class="input-group-addon"><span class="glyphicon glyphicon-time"></span> </span>
                                            </div>


                                        </td>
                                        <td data-title="à">
                                            <div class='input-group date'>
                                                <input type='text' name="qouiqui[Sam][end]" class="form-control  timepick2"/><span class="input-group-addon"><span class="glyphicon glyphicon-time"></span> </span>
                                            </div>

                                        </td>

                                    </tr>
                                    <tr class="day-sunday" data-open="9:00" data-close="17:00">
                                        <td data-title="Jours">Dimanche</td>
                                        <td data-title="du">
                                            <div class='input-group date'>
                                                <input type='text' name="qouiqui[Dim][start]" class="form-control  timepick2"/><span class="input-group-addon"><span class="glyphicon glyphicon-time"></span> </span>
                                            </div>


                                        </td>
                                        <td data-title="à">
                                            <div class='input-group date'>
                                                <input type='text' name="qouiqui[Dim][end]" class="form-control  timepick2"/><span class="input-group-addon"><span class="glyphicon glyphicon-time"></span> </span>
                                            </div>

                                        </td>

                                    </tr>
                                    </tbody>
                                </table>
                            </div>


                            <div class="form-group row element-form">
                                <div class="col-md-12">
                                    <label class="control-label"> Choisissez une image pour le service</label>
                                </div>
                                <div class="col-sm-9 DefaultIcon">
                                    {% for iconservice in DirAssetParcour(asset('uploads/image/iconService/'~attributes.category.slug|lower~'/SVG/')) %}
                                        <a href="#" onclick="javascript:;">
                                            <img width="50px" height="50px" src="{{ asset('uploads/image/iconService/') }}{{ attributes.category.slug|lower~'/SVG/' }}{{ iconservice }}" alt="">
                                        </a>
                                    {% endfor %}
                                </div>
                            </div>

                            <input type="text" id="icon-filtre-2" class="icon-filtre form-control name_service" placeholder="recherche Votre Image">
                            <input type="hidden" name="serviceIcon" class="form-control name_service" placeholder="recherche Votre Image">
                            <button type="button" class="btn btn-primary btn-lg btn-block"><i class="fa fa-search" aria-hidden="true"></i></button>
                            <div class=" pull-right col-sm-9 icon_list">
                                {{ include('@User/template/fontawesomeIcons.html.twig') }}
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class=" btn btn-danger button-annuler" data-dismiss="modal">Annuler</button>
                <button type="submit" class=" btn btn-primary button-default " data-target="modifier" data-selector="ajout_services_frm">Ajouter</button>
            </div>
        </div>
    </div>
</div>

