                                </select>
                            </div>
                            <div class="form-group">
                                <select class="selectpicker form-control js-example-basic-multiple" id="souscategories" name="souscategorie[]" multiple="multiple">
                                    <option selected disabled class="Renault Peugeot">Equipement</option>

                                </select>
                            </div>
                            {#<div class=" form-group">#}
