            </div>
            <div class="modal-body">
                <div class="row">
                    <form id="addequipement" method="POST"
                          action="{{ path('module_spa_add_offre',{"idqq":attributes.quoiqui.id}) }}"
                          enctype="multipart/form-data">
                        {#{% if mission.event is defined %}#}
                        {#{% set count= mission.event|length %}#}

                        {#{% if count >0 %}#}

                        {#{% for a in  attributes.theme.event %}#}
