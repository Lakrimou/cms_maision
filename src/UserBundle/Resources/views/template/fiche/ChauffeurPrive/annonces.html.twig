<div class="modal fade" id="listOffres" role="dialog">
    <div class="modal-dialog modal-lg">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Liste des véhicules</h4>

            </div>
            <div class="modal-body">
                <div class="container">
                    <table class=" dataTable table table-striped table-bordered  " cellspacing="0" width="100%">
                        <thead>
                        <tr>
                            <th>Image</th>
                            <th>véhicules</th>

                            <th>Modifier</th>
                            <th>Supprimer</th>
                        </tr>
                        </thead>

                        <tbody>
                        {% if mission .event is defined %}
                            {% set arrayEvent=mission .event %}
                            {% for b in arrayEvent|sort|reverse %}

                                <tr data-td="{{ b.idOffre }}">

                                    <td>

                                        <img src="{{ asset('uploads/image/') }}{{ attributes.quoiqui.user.id }}/{{ attributes.quoiqui.id }}/event/{{ b.idOffre ~'.jpg' }}"
                                             data-pin-nopin="true" style="  max-width:  20%;">

                                    </td>
                                    <td> {{ b.souscategorie.0 }}</td>


                                    <td>

                                        <a href="#" class=" btn btn-info a-btn-slide-text edit-offre"
                                           data-serv="{{ b.idOffre }}"
                                           data-id="{{ attributes.quoiqui.id }}" data-toggle="modal"
                                           data-target="#editambulances">

                                            <span class="glyphicon glyphicon-edit" aria-hidden="true"></span>
                                            <span><strong>Modifier</strong></span>
                                        </a>

                                    </td>


                                    <td>

                                        <a href="{{ path('module_spa_delate_events',{"idqq": attributes.quoiqui.id,"idevent": b.idOffre }) }}"
                                           class=" btn btn-danger a-btn-slide-text service_suprim"
                                           onclick="clickOffConfirmed = confirm('Vous voulez le  supprimer ?');">
                                            <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
                                            <span><strong>Supprimer</strong></span>
                                        </a>
                                    </td>

                                </tr>
                                {#{% endfor %}#}
                            {% endfor %}
                        {% endif %}
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#addEvent"
                >Ajouter un véhicule
                </button>
                <button type="button" class="btn btn-danger" data-dismiss="modal">Fermer</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="addEvent" role="dialog">
    <div class="modal-dialog ">

