                        {#{% set comp=a.idOffre+1 %}#}
                        {#{% endif %}#}
                        {#{% endfor %}#}

                        {#{% else %}#}

                        {#<input type="hidden" name="idOffre" value="1">#}
                        {#{% endif %}#}
                        {#{% else %}#}

                        {#<input type="hidden" name="idOffre" value="1">#}
                        {#{% endif %}#}
                        <input type="hidden" name="idOffre" value="{{ uniqueId() }}">

                        <div class="col-md-12">
                            <div class="form-group">
                                <select class="selectpicker form-control" id="categories" name="categorie">
