                            {#class="form-control" name="ServSpa">#}
                            {#</div>#}


                            <div class="form-group">
                                <label class="col-md-12">Description</label>
                                <textarea placeholder="Entrer la description.." rows="3"
                                          class="form-control textarea "
                                          name="description" required="required"></textarea>
                            </div>

                            <div class=" form-group">
                                <label class="col-md-12">Choisissez une photo</label>
                                <input autocomplete="off" class="form-control "
                                       name="file" type="file"
                                       placeholder="choisissez_image" data-items="8"/>

                            </div>

                        </div>
                        {#<div class="col-md-6">#}
                        {##}
                        {#</div>#}


                    </form>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Fermer</button>
                <button type="submit" class=" btn btn-primary button-default" data-target="modifier"
                        data-selector="addequipement">
                    Enregistrer
                </button>
            </div>
        </div>

    </div>
</div>


<div class="modal fade" id="editambulances" role="dialog">
    <div class="modal-dialog ">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Modifier le véhicule</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <form id="editambulance" method="POST"
                          action="{{ path('module_spa_update_event',{"idqq":attributes.quoiqui.id}) }}"
                          enctype="multipart/form-data">


                        <input id="idEvent" type="hidden" name="idOffre" value="">

                        <div class="col-md-12">

                            <div class="form-group">
                                <select class="selectpicker form-control" id="editcategories" name="categorie">
                                    <option selected disabled>Type de Véhicule</option>
                                    {%  for mark in marks %}
                                        <option value="{{ mark.id }}"> {{ mark.name }}</option>
                                    {% endfor %}
                                </select>
                            </div>
                            <div class="form-group">

                                <select id="sousCategoriesHidden" style="display: none;">
                                    <option selected disabled>Equipement</option>
                                    {%  for model in models %}
                                        <option value="{{ model.id }}" data-cat="{{ model.mark.id}}"> {{ model.name }}</option>
                                    {% endfor %}
                                </select>

                                <select class="selectpicker form-control js-example-basic-multiple editequip" id="editsouscategories" name="souscategorie[]" multiple="multiple">
                                </select>

                            </div>

                            <div class="form-group">
                                <label class="col-md-12">Description</label>
                                <textarea id="descr" placeholder="Entrer la description.." rows="3"
                                          class="form-control textarea   description-edit"
                                          name="description" required="required"></textarea>
                            </div>


                            <div class=" form-group">
                                <label class="col-md-12">Choisissez une photo</label>
                                <input autocomplete="off" class="form-control "
                                       name="file" type="file"

                                       placeholder="choisissez_image" data-items="8"/>

                            </div>
                        </div>


                    </form>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Fermer</button>
                <button type="submit" class="btn btn-primary button-default" data-target="modifier"
                        data-selector="editambulance">
                    Enregistrer
                </button>
            </div>
        </div>

    </div>
</div>


