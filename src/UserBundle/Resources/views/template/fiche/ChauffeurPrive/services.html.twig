                                    {% endif %}
                                {% endif %}
                            </div>
                            <div style="padding: 10px;">
                                <div class="col-lg-8 col-md-8">
                                    <div id="divMapp" style="width: 100%; height: 400px;">

                                    </div>
                                </div>
                                <div class="col-lg-4 col-md-4" id="divRoutee" style="height: 400px; overflow: auto;"></div>
                            </div>
                            <div class="loading text-center">
                                <i class="fa fa-spinner fa-pulse fa-3x fa-fw"></i>
                                <span class="sr-only">Loading...</span>
                            </div>
                            <div class="text-center success" style="height: 200px; padding: 15px; font-size: 25px;">
                                <i class="fa fa-check-circle-o fa-3x" aria-hidden="true"></i>
                            </div>
                        </form>
                    {% endif %}
                </div>
            </div>
            <div class="modal-footer" id="footer_rappeler" style="margin-top: 50px;">
                <button type="button" class="btn btn-primary " id="estimationn">Estimer</button>
                <button type="button" class="btn btn-danger" data-dismiss="modal">Fermer</button>

            </div>

        </div>

    </div>
</div>



