                                    <input type="text" id="adrArrr" value="" class="form-control">
                                    <input type="button" class="btn btn-lg" value="recherche sur la carte" onclick="rechercherr('adrArrr',false)">
                                </div>
                                {% set position = 0 %}
                                {% if mission.service is defined %}
                                    {% set arraymission=mission.service %}
                                    {% for b in arraymission|sort|reverse %}
                                        {% if b.vehicule is defined %}
                                            <div class="form-group">
                                               {# {{ dump(b) }}#}
                                                <label for="voiture" class="control-label">Voitures disponibles</label>
                                                <select name="voiture" id="voiture" class="form-control">
                                                    {% set compteur = 0 %}
                                                    {% set compteurPrice = 0 %}
                                                    {% for voiture in b.vehicule %}
                                                        {% if b.carPrice is defined %}
                                                            {% for price in b.carPrice %}
                                                                {% if loop.index0 == compteur %}
                                                                    {% if b.currency is defined %}
                                                                        {% for currence in b.currency %}
                                                                            {% if loop.index0 == compteur %}
                                                                                <option value="{{ voiture }}" data-price="{{ price }}" data-currency="{{ currence }}">{{ voiture }}</option>
                                                                            {% endif %}
                                                                        {% endfor %}
                                                                    {% endif %}
                                                                {% endif %}
                                                                {% set compteurPrice = compteurPrice + 1 %}
                                                            {% endfor %}
                                                        {% endif %}
                                                        {% set compteur =  compteur + 1 %}
                                                    {% endfor %}
                                                </select>
                                            </div>
                                        {% endif %}
                                        {#{% if b.currency is defined %}
                                            {% for currence in b.carPrice %}
                                                {% if loop.index0 == position %}
                                                    <div class="form-group">
