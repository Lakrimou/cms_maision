                                <div class="">

                                    <div class="">

                                        <form action="{{ path('change_avatar',{ 'qq_id': qouiqui.id }) }}" method="POST" accept="image/gif, image/jpeg, image/png" enctype="multipart/form-data">
                                            <span class="text-center"> Choisir Votre Logo</span>
                                            <input class="btn-default" type="file" name="fileToUpload" id="fileToUpload"><br>
                                            <input type="submit" value="Envoyer les images" class="btn btn-primary" name="submit" style="width: 100%;">
                                        </form>
                                        <hr>

                                        {% if asset_exists(path) %}
                                            <form action="{{ path('supp_avatar',{'qq_id': qouiqui.id}) }}" id="suplogo" method="POST">
                                                <p class="text-center">
                                                <center>
                                                    <img class="logologo img-responsive" src="{{ asset('uploads/image/') }}{{ qouiqui.user.id }}/{{ qouiqui.id }}/avatar/avatar.jpg" alt="">
                                                </center>
                                                </p>
                                                <input type="submit" value="Supprimer le logo" class="sup-logo theme-btn btn-danger btn-style-one blue marg40" name="submit" style="width: 100%;"><br>
                                            </form>
                                        {% else %}
                                            <h4 class="pad15 text-center">
                                                Vous n'avez pas encore de logo professionnel !! <br>
                                                <button type="button" class="btn btn btn-success margin15  btn-lg shake-little shake-constant" data-toggle="modal" data-target="#logopropose">Proposition de Logo</button>
                                            </h4>
                                        {% endif %}

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="submit" class="btn btn-primary " data-target="modifier" data-selector="edit_info">
                        Enregistrer
                    </button>
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Annuler</button>
                </div>
            </div>
        </div>
    </div>
