                            <li role="presentation"><a href="#browseTab" aria-controls="browseTab" role="tab" data-toggle="tab">Modifier le logo</a></li>
                        </ul>
                        <!-- Tab panes -->
                        <div class="tab-content">
                            <div role="tabpanel" class="tab-pane active" id="uploadTab">
                                <form id="edit_info" method="POST" action="{{ path('module_propo') }}">

                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <label for="adminbundle_ngn_qouiqui_firstName">Nom de votre société *</label>
                                                <input type="text" value="{{ qouiqui.firstName }}" id="adminbundle_ngn_qouiqui_firstName" name="qouiqui[firstName]" required="required" class="form-control" minlength="3" maxlength="120">
                                            </div>
                                        </div>

                                    </div>

                                    <div class="row">
                                        <div class="col-md-12">
                                            <h5>Description</h5>
                                            <textarea id="det" name="editor" value="">{{ qouiqui.details }}</textarea>
                                        </div>
                                        <div class="">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label for="exampleInputEmail2">Ville</label>
                                                    <select id="aboutEditVille" name="qville[ville]" class="form-control select22" required style="width: 100%;"></select>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label for="exampleInputEmail2">Délegation</label>
                                                    <select id="aboutEditDelegation" name="qville[delegation]" class="form-control select22" required style="width: 100%;"></select>
                                                    <select id="aboutEditDelegationHidden" style="display: none;"></select>
                                                </div>
                                            </div>
                                        </div>


                                        <div class="col-md-6">
                                            <h5>Adresse</h5>
                                            <input type="text" name="qouiqui[address1]" class="form-control" value="{{ qouiqui.address1 }}">
                                            <input type="text" name="qouiqui[id]" class="hidden" value="{{ qouiqui.id }}">
                                        </div>
                                        <div class="col-md-6">
                                            <h5>Adresse additionelle</h5>
                                            <input type="text" name="qouiqui[address2]" class="form-control" value="{{ qouiqui.adress2 }}">
                                        </div>
                                        <div class="col-md-6">
                                            <h5>Numéro du télephone </h5>
                                            <input type="text" name="qouiqui[phone]" class="form-control" value="{{ qouiqui.phone }}">
                                        </div>
                                        <div class="col-md-6">
                                            <h5>Deuxieme Numéro </h5>
                                            <input type="text" name="qouiqui[phone2]" class="form-control" value="{{ qouiqui.phone2 }}">
                                        </div>
                                        <div class="col-md-6">
                                            <h5>Adresse email </h5>
                                            <input type="text" name="qouiqui[mail]" class="form-control" value="{{ qouiqui.mail }}">
                                        </div>
                                        <div class="col-md-6">
                                            <h5>Site Web </h5>
                                            <input type="text" name="qouiqui[website]" class="form-control" value="{{ qouiqui.website }}">
                                        </div>


                                        <div class="col-md-12">
                                            <h5>Lien de la video (Youtube, Dailymotion ou Vimeo)</h5>
                                            {% if qouiqui.video %}
                                                <div class="input-group">
                                                    <input type="text" name="qouiqui[video]" id="inputUrlVideo" class="form-control" value="{{ qouiqui.video }}">
                                                    <span class="input-group-addon" id="deleteUrlVideo" data-toggle="tooltip" data-placement="bottom" title="Supprimer le lien">X</span>
                                                </div>
                                            {% else %}
                                                <input type="text" name="qouiqui[video]" id="inputUrlVideo" class="form-control" value="{{ qouiqui.video }}">
                                            {% endif %}
                                        </div>


                                        <div class="col-md-12">
                                            <h5>Mots Clés</h5>
                                            <input name="tags" type="text" class="form-control tokenfield" id="tokenfield" value="{% if data.tags is defined %} {{ data.tags }} {% endif %}"/>
                                        </div>


                                        <hr class="clear-fix">
                                        <div class="container-fluid">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <h5>Horaires</h5>

                                                    <div class="col-md-6 ">
                                                        <h5>Ouvert au cours de la semaine du</h5>
                                                        {#<input type="text" name="qouiqui[opening]" class="form-control"  value="{{ qouiqui.opening }}">#}
                                                        <div class="bfh-timepicker" data-mode="12h" data-time="08:00" data-name="qouiqui[opening][week][start]">
                                                        </div>
                                                        <h5>à</h5>
                                                        <div class="bfh-timepicker" data-mode="12h" data-time="18:00" data-name="qouiqui[opening][week][end]"></div>
                                                    </div>
                                                    <div class="col-md-6 ">
                                                        <h5>Fermé du</h5>
                                                        {#<input type="text" name="qouiqui[opening]" class="form-control"  value="{{ qouiqui.opening }}">#}
                                                        <div class="bfh-timepicker" data-mode="12h" data-time="13:00" data-name="qouiqui[opening][close][start]"></div>
                                                        <h5>à</h5>
                                                        <div class="bfh-timepicker" data-mode="12h" data-time="14:00" data-name="qouiqui[opening][close][end]"></div>
                                                    </div>
                                                    <div class="col-md-6 ">
                                                        <div class="checkbox checkbox-primary">
                                                            <input id="checkbox2" type="checkbox" name="qouiqui[opening][weekendsamedi][open]" value="true">
                                                            <label for="checkbox2">
                                                                Samedi du
                                                            </label>
                                                        </div>
                                                        {#<input type="text" name="qouiqui[opening]" class="form-control"  value="{{ qouiqui.opening }}">#}
                                                        <div class="bfh-timepicker" data-mode="12h" data-time="13:00" data-name="qouiqui[opening][weekendsamedi][start]">
                                                        </div>
                                                        <h5>à</h5>
                                                        <div class="bfh-timepicker" data-mode="12h" data-time="14:00" data-name="qouiqui[opening][weekendsamedi][end]">
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6 ">
                                                        <div class="checkbox checkbox-primary">
                                                            <input id="checkbox3" name="qouiqui[opening][weekenddimanche][open]" value="true" type="checkbox">
                                                            <label for="checkbox3">
                                                                Dimanche du
                                                            </label>
                                                        </div>
                                                        {#<input type="text" name="qouiqui[opening]" class="form-control"  value="{{ qouiqui.opening }}">#}
                                                        <div class="bfh-timepicker" data-mode="12h" data-time="13:00" data-name="qouiqui[opening][weekenddimanche][start]">
                                                        </div>
                                                        <h5>à</h5>
                                                        <div class="bfh-timepicker" data-mode="12h" data-time="14:00" data-name="qouiqui[opening][weekenddimanche][end]">
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <h5>Réseaux sociaux</h5>
                                                    <div class="col-md-12">
                                                        <h5>Facebook </h5>
                                                        <input type="text" name="qouiqui[rs][facebook]" class="form-control" value="{% if sociaux!= null %}{{ sociaux.facebook }} {% endif %}">
                                                    </div>
                                                    <div class="col-md-12">
                                                        <h5>Tweeter </h5>
                                                        <input type="text" name="qouiqui[rs][twitter]" class="form-control" value="{% if sociaux != null %}{{ sociaux.twitter }}{% endif %}">
                                                    </div>
                                                    <div class="col-md-12">
                                                        <h5>Google Plus </h5>
                                                        <input type="text" name="qouiqui[rs][google]" class="form-control" value="{% if sociaux!= null %}{{ sociaux.google }}{% endif %}">
                                                    </div>
                                                    <div class="col-md-12">
                                                        <h5>linkedin</h5>
                                                        <input type="text" name="qouiqui[rs][linkedin]" class="form-control" value="{% if sociaux!= null %}{{ sociaux.linkedin }}{% endif %}">
                                                    </div>
                                                </div>
                                            </div>
                                            <hr>
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label for="adminbundle_ngn_qouiqui_numRegistre">N° Registre de Commerce (optionnel)</label>
                                                        <input type="text" value="{{ qouiqui.numRegistre }}" id="adminbundle_ngn_qouiqui_numRegistre" name="qouiqui[numRegistre]" class="form-control" minlength="3" maxlength="40" placeholder="ex: B186502000">
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label for="adminbundle_ngn_qouiqui_numPatente">Matricule fiscal (optionnel)</label>
                                                        <input type="text" value="{{ qouiqui.numPatente }}" id="adminbundle_ngn_qouiqui_numPatente" name="qouiqui[numPatente]" class="form-control" minlength="3" maxlength="40" placeholder="ex: 0031458D">
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                        <hr class="clear-fix">
                                        <br>

                                    </div>
                                </form>
                                <blockquote style="border-left: 5px solid #397cb2;">

                                    <span class="text-center text-uppercase CEOTEXT" style="font-size: 30px">VOTRE STRUCTURE DE RÉFÉRENCEMENT GOOGLE EST PRÊTE MAINTENANT ET  N'EST PAS ENCORE ACTIVEE , DÉMARRER VOTRE ABONNEMENT SEO</span>
                                    <hr>
                                    <pre>
&lt;title&gt;{{ qouiqui.firstname }}, {{ category.libelle }}{% for location in qouiqui.qvilles %}{% if location.delegation %}, {{ location.delegation.libelle }}{% endif %}{% if location.ville %},   {{ location.ville.libelle }}{% endif %}{% endfor %}&lt;/title&gt;

&lt;meta name="description" content="{{ qouiqui.shortDescription }}"/&gt;

&lt;meta name='keywords' content='{% if data.tags is defined %} {{ data.tags }} {% endif %}'&gt;

&lt;meta property='og:title' content='{{ qouiqui.firstname }}, {{ category.libelle }}{% for location in qouiqui.qvilles %}{% if location.delegation %}, {{ location.delegation.libelle }}{% endif %}{% if location.ville %},   {{ location.ville.libelle }}{% endif %}{% endfor %}'/&gt;

&lt;meta property='og:description' content='{{ qouiqui.shortDescription }}'/&gt;
                                    </pre>

                                </blockquote>


                                <button class="btn btn-success btn-lg h-100 booster-shak">BOOSTER MAINTENANT VOTRE POSITIONNEMENT À PARTIR DE 199DT HT / MOIS</button>


                                <style>
                                    blockquote {
                                        margin-top: 15px;
                                        pointer-events: none;
                                        user-select: none;
                                        border: 1px blue dashed;
                                    }

                                    xmp {
                                        overflow: scroll;
                                    }

                                </style>

                            </div>
