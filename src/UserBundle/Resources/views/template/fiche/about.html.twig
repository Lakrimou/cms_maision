<!-- WELCOME -->


<section id="section-apropos" class="padding">
    <div class="ooo">

        <span class="o bg" data-toggle="tooltip" data-placement="left" title="Couleur d'arrière plan!"></span>
        <span class="o mt" data-toggle="tooltip" data-placement="left" title="Motif d'arrière plan!"></span>
        <span class="o an" data-toggle="tooltip" data-placement="left" title="Animation d'arrière plan!"></span>

    </div>
    <div class="container">


        {% if  connected %}
            <div class="row">
                <div data-step="2" data-intro="ici vous pouvez modifier vos information de votre fiche !" class="pull-right margin-bott-40">
                    <button type="button" class="theme-btn button11 btn-style-one pull-right" data-toggle="modal" data-target="#editpropo">modifier les information</button>
                </div>
            </div>
        {% endif %}


        <div class="row row-about vertical-align">
            <div class="column col-md-6 col-sm-12 col-xs-12 about-logo hidden-xs hidden-sm">
                {#<div class="video-box">#}
                {#<figure class="image logo">#}
                {% set path =  asset('uploads/image/')~qouiqui.user.id ~'/'~ qouiqui.id ~'/avatar/avatar.jpg' %}
                {% if qouiqui.video  and urlToEmbded(qouiqui.video) %}
                    <div class="embed-responsive embed-responsive-16by9" style="margin-top: 8px;">
                        <iframe class="embed-responsive-item" src=" {{ urlToEmbded(qouiqui.video) }}" frameborder="0" allowfullscreen></iframe>
                    </div>

                    {#<div class="col-md-6">#}
                    {#<iframe width="1280" height="720" ></iframe>#}
                    {#</div>#}
                {% else %}
                    {% if asset_exists(path) %}
                        <div>
                            <img src="{{ asset('uploads/image/') }}{{ qouiqui.user.id }}/{{ qouiqui.id }}/avatar/avatar.jpg" class="img-responsive">
                        </div>
                    {% else %}
                        <div style="width:300px;height:300px;margin: 0 auto;">
                            <h1 class="about-title-logo tlt" data-in-effect="flash" data-out-effect="flash" data-out-shuffle="true">{{ qouiqui.firstName }}</h1>
                        </div>
                        <style>
                            .vertical-align {
                                align-items: center;
                            }
                        </style>
                    {% endif %}
                {% endif %}
                {#</figure>#}
                {#</div>#}
            </div>

            <!--Content Column / Fact Counter Two-->
            <div class="content-column col-md-6 col-sm-12 col-xs-12">
                <div class="inner-box">

                    {% set sociaux = jsonDecod(qouiqui.sociaux) %}

                    <h2 class="text-uppercase">Bienvenue À <span class="color_red tlt" data-in-effect="fadeIn" data-out-effect="fadeOut" data-out-shuffle="true">{{ qouiqui.firstname }}</span></h2>
                    <div class="text description">
                        {{ qouiqui.details|raw }}
                    </div>

                    <!--Fact Counter Two-->
                    <div class="fact-counter">
                        <div class="clearfix">


                            <div class="info-box norow">
                                <div class="col-md-6">
                                    <ul class="clearfix">
                                        <li class="address">
                                            <div class="icon_propo"><span class="fa fa-home color_red"></span></div>
                                            <div class="marg-left-40">
                                                <h5>Adresse</h5>
                                                {{ qouiqui.address1 }}{% if qouiqui.adress2 %}, {{ qouiqui.adress2 }}{% endif %}
                                                {% for location in qouiqui.qvilles %}
                                                    {% if location.delegation %}, {{ location.delegation.libelle }}{% endif %}
                                                    {% if location.ville %}, {{ location.ville.libelle }}{% endif %}
                                                {% endfor %}
                                            </div>
                                        </li>
                                        <li class="contact-info">
                                            <div class="icon_propo"><span class="fa fa-phone color_red" aria-hidden="true"></span></div>
                                            <div class="marg-left-40">
                                                <h5>Téléphone</h5>
                                                {{ qouiqui.phone }}  {% if qouiqui.phone2 %}/ {{ qouiqui.phone2 }}{% endif %}
                                            </div>
                                        </li>
                                        <li class="contact-info">
                                            <div class="icon_propo"><i class="fa fa-clock-o color_red" aria-hidden="true"></i></div>
                                            <div class="marg-left-40">

                                                <h5>Horaires</h5>
                                                {% if horaire != null %}
                                                    Ouvert du {{ horaire.week.start }} à {{ horaire.week.end }}<br>
                                                    Fermé du {{ horaire.close.start }} à {{ horaire.close.end }}<br>

                                                    Samedi  {% if horaire.weekendsamedi.open is defined %} du {{ horaire.weekendsamedi.start }} à {{ horaire.weekendsamedi.end }} {% else %}Fermé{% endif %}<br>
                                                    Dimanche{% if horaire.weekenddimanche.open is defined %} du {{ horaire.weekenddimanche.start }} à {{ horaire.weekenddimanche.end }}{% else %} Fermé {% endif %}
                                                {% else %}
                                                    Pas D'horaire
                                                {% endif %}
                                            </div>
                                        </li>
                                    </ul>
                                </div>
                                <div class="col-md-6">
                                    <ul class="clearfix">
                                        <li class="address">
                                            <div class="icon_propo"><span class="fa fa-puzzle-piece color_red"></span></div>
                                            <div class="marg-left-40">
                                                <h5>Activité</h5>
                                                {{ category.libelle }}
                                            </div>
                                        </li>
                                        <li class="contact-info">
                                            <div class="icon_propo"><span class="fa fa-envelope-o color_red"></span></div>
                                            <div class="marg-left-40">
                                                <h5>Email</h5>
                                                {{ qouiqui.mail }}

                                            </div>

                                        </li>
                                        <li class="contact-info">
                                            <div class="icon_propo"><span class="fa fa-globe color_red"></span></div>
                                            <div class="marg-left-40">
                                                <h5>Website</h5>
                                                {{ qouiqui.website }}
                                            </div>
                                        </li>

                                    </ul>
                                </div>

                                <div class="clearfix"></div>
                                <hr>
                                <div class="col-md-6 pull-right row">

                                    <div class="btn-group" role="group" aria-label="...">
                                        <a href="#section-contact" class="btn_fill_about btn btn-default">Contactez Nous</a>
                                        <a href="http://maps.google.com?q={{ qouiqui.geoLat }},{{ qouiqui.geoLong }}" target="_blank" class="btn_fill_about btn btn-default">Y aller</a>
                                    </div>


                                    <div class="hidden-lg hidden-sm hidden-md pad-top imggoogle" style="padding-left:0;padding-top: 7px;">
                                        <a target="_blanck" href="http://maps.google.com?q={{ qouiqui.geoLat }},{{ qouiqui.geoLong }}">
                                            <img style="width:100%" src="https://maps.googleapis.com/maps/api/staticmap?zoom=17&size=600x300&maptype=roadmap
                                            &markers=color:blue%7C
                                            label:webmaster-media%7C{{ qouiqui.geoLat }},{{ qouiqui.geoLong }}
                                            &key=AIzaSyBTdMydc9PUE0gPkcgjRZXOuFPXc5BUSrg">
                                        </a>
                                    </div>
                                </div>

                                <div class="col-md-6 rs" style="display: -webkit-box;">
                                    <a href="{% if sociaux.facebook is defined %}{{ sociaux['facebook'] }} {% endif %}" target="_blank"><img src="https://cdn3.iconfinder.com/data/icons/capsocial-round/500/facebook-32.png" alt=""></a>
                                    <a href="{% if sociaux.tweeter is defined %}{{ sociaux['tweeter'] }}{% endif %}" target="_blank"><img src="https://cdn3.iconfinder.com/data/icons/capsocial-round/500/twitter-32.png" alt=""></a>
                                    <a href="{% if sociaux.linkedin is defined %}{{ sociaux['linkedin'] }}{% endif %}" target="_blank"><img src="https://cdn3.iconfinder.com/data/icons/capsocial-round/500/linkedin-32.png" alt=""></a>
                                    <a href="{% if sociaux.google is defined %}{{ sociaux['google'] }}{% endif %}" target="_blank"><img src="https://cdn3.iconfinder.com/data/icons/capsocial-round/500/google-32.png" alt=""></a>
                                </div>

                                {% if qouiqui.video  and urlToEmbded(qouiqui.video) %}
                                    <div class="hidden-lg hidden-sm hidden-md pad-top imggoogle" style="padding-left:0;padding-top: 19px;">
                                        <div class="embed-responsive embed-responsive-16by9">
                                            <iframe class="embed-responsive-item" src=" {{ urlToEmbded(qouiqui.video) }}" frameborder="0" allowfullscreen></iframe>
                                        </div>
                                    </div>
                                {% endif %}

                            </div>


                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
    {% if not is_mobile() %}
        <div class="paralax-icon">
            <div class="particle elem1 anim-delay1"></div>
            <div class="particle elem2 anim-delay5"></div>
            <div class="particle elem3 anim-delay3"></div>
            <div class="particle elem4 anim-delay8"></div>
            <div class="particle elem5 anim-delay9"></div>
            <div class="particle elem6 anim-delay2"></div>
            <div class="particle elem7 anim-delay4"></div>
            <div class="particle elem8 anim-delay3"></div>
            <div class="particle elem9 anim-delay10"></div>
            <div class="particle elem8 anim-delay11"></div>
            <div class="particle elem6 anim-delay12"></div>
            <div class="particle elem1 anim-delay14"></div>
            <div class="particle elem13 anim-delay16"></div>
            <div class="particle elem17 anim-delay18"></div>
            <div class="particle elem2 anim-delay6"></div>
            <div class="particle elem16 anim-delay9"></div>
            <div class="particle elem8 anim-delay3"></div>
            <div class="particle elem9 anim-delay10"></div>
            <div class="particle elem18 anim-delay11"></div>
            <div class="particle elem11 anim-delay12"></div>
            <div class="particle elem12 anim-delay14"></div>
            <div class="particle elem13 anim-delay16"></div>
            <div class="particle elem14 anim-delay18"></div>
            <div class="particle elem15 anim-delay6"></div>
            <div class="particle elem16 anim-delay9"></div>
            <div class="particle elem17 anim-delay7"></div>
            <div class="particle elem18 anim-delay6"></div>
            <div class="particle elem19 anim-delay3"></div>
        </div>
    {% endif %}
</section>
