    function validateForm(id) {

        if (jQuery(id + ' [required]').length > 0) {
            valid = true;

            jQuery(id + ' [required]').each(function () {
                if ($(this).val() === '') {
                    $(this).addClass("error");
                    valid = false;
                    return false;
                } else {
                    valid = true;
                    $(this).removeClass("error");
                }
            });
            if (valid == true)
                return submit(id);
            else
                return false;
        } else {
            return submit(id);
        }
    }
