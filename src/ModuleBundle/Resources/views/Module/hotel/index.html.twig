        {#-------------------------annimation background--------------------------------#}
        <script>


            function chnageicon(iconlink, div_name) {

                for (var i = 1; i < 29; i++) {

                    $(div_name + " .elem" + i).css('background-image', 'url(' + iconlink + ')')

                }
            }
            $('[set_icon]').click(function () {

                data_pour = $(this).parent().attr('data-pour');
                section = $(this);
                srcicon = section.find('img').attr('src')

                if (data_pour == 'background-horaires') {
                    chnageicon(srcicon, '#horaires');
                    $('#animation_horaires').val(srcicon);
                }
                else if (data_pour == 'background-propo') {
                    chnageicon(srcicon, '#apropos');
                    $('#animation_about').val(srcicon);
                }
                else if (data_pour == 'background-service') {
                    chnageicon(srcicon, '.services-section');
                    $('#annimation_service').val(srcicon);
                }
                else if (data_pour == 'background-annonce') {
                    chnageicon(srcicon, '#events');
                    $('#animation_annonce').val(srcicon);
                }
                else if (data_pour == 'background-gallery') {
                    chnageicon(srcicon, '.news-section');
                    $('#annimation_gallery').val(srcicon);
                }
                else if (data_pour == 'background-gallery') {
                    chnageicon(srcicon, '.main-footer');
                    $('#annimation_footer').val(srcicon);
                    console.log($('#annimation_footer').val(srcicon))
                }

            });

