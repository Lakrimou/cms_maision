    function submit(id) {
        return jQuery(id).submit()
    }

    function validateForm(id) {

        if (jQuery(id + ' [required]').length > 0) {
            valid = true;

            jQuery(id + ' [required]').each(function () {
                if ($(this).val() === '') {
                    $(this).addClass("error");
                    valid = false;
                    return false;
                } else {
                    valid = true;
                    $(this).removeClass("error");
                }
            });
            if (valid == true)
                return submit(id);
            else
                return false;
        } else {
            return submit(id);
        }
    }
    ;
    jQuery("[data-target='modifier']").click(function () {
        var isValid = true;
        id = $(this).attr("data-selector");
        id = "#" + id;
        validateForm(id);
    });

</script>

