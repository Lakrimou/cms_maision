<!-- Modal -->
<div id="editpropo" class="modal fade" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->

        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Modifier les information</h4>
            </div>
            <div class="modal-body">
                <div role="tabpanel">
                    <!-- Nav tabs -->
                    <ul class="nav nav-tabs" role="tablist">
                        <li role="presentation" class="active"><a href="#uploadTab" aria-controls="uploadTab" role="tab"
                                                                  data-toggle="tab">mise a jour votre info</a>
                        </li>
                        <li role="presentation"><a href="#browseTab" aria-controls="browseTab" role="tab"
                                                   data-toggle="tab">modifier le logo</a>
                        </li>
                    </ul>
                    <!-- Tab panes -->
                    <div class="tab-content">
                        <div role="tabpanel" class="tab-pane active" id="uploadTab">
                            <form id="edit_info" method="POST" action="{{ path('module_spa_about') }}">

                                <div class="row">
                                    <div class="col-md-12">
                                        <h4>Description</h4>
                                        <textarea id="det" name="editor" value="">{{ qouiqui.details }}</textarea>
                                    </div>

                                    <div class="col-md-6">
                                        <h4>Address</h4>
                                        <input type="text" name="qouiqui[address1]" class="form-control"
                                               value=" {{ qouiqui.address1 }}">
                                        <input type="text" name="qouiqui[id]" class="hidden" value="{{ qouiqui.id }}">
                                    </div>

                                    <div class="col-md-6">
                                        <h4>Address additionelle</h4>
                                        <input type="text" name="qouiqui[address2]" class="form-control"
                                               value="{{ qouiqui.adress2 }}">
                                    </div>

                                    <div class="col-md-6">
                                        <h4>Numéro du télephone </h4>
                                        <input type="text" name="qouiqui[phone]" class="form-control"
                                               value=" {{ qouiqui.phone }}">
                                    </div>

                                    <div class="col-md-6">
                                        <h4>Deuxieme Numéro </h4>
                                        <input type="text" name="qouiqui[phone2]" class="form-control"
                                               value="{{ qouiqui.phone2 }}">
                                    </div>

                                    <div class="col-md-6">
                                        <h4>Address email </h4>
                                        <input type="text" name="qouiqui[mail]" class="form-control"
                                               value="{{ qouiqui.mail }}">
                                    </div>

                                    <div class="col-md-6">
                                        <h4>Site Web </h4>
                                        <input type="text" name="qouiqui[website]" class="form-control"
                                               value="{{ qouiqui.website }}">
                                    </div>

                                    <div class="col-md-12">
                                        <h4>Horaire</h4>
                                        <div class="col-md-6 nopadd">
                                            <h5>Ouvert Matin de</h5>
                                            {#<input type="text" name="qouiqui[opening]" class="form-control"  value="{{ qouiqui.opening }}">#}
                                            <div class="bfh-timepicker" data-mode="12h" data-time="08:00"
                                                 data-name="qouiqui[opening][week1][start]">
                                            </div>
                                            <h5>à</h5>
                                            <div class="bfh-timepicker" data-mode="12h" data-time="18:00"
                                                 data-name="qouiqui[opening][week1][end]">
                                            </div>
                                        </div>

                                        <div class="col-md-6 nopadd">
                                            <h5>Ouvert Après-midi de</h5>
                                            {#<input type="text" name="qouiqui[opening]" class="form-control"  value="{{ qouiqui.opening }}">#}
                                            <div class="bfh-timepicker" data-mode="12h" data-time="13:00"
                                                 data-name="qouiqui[opening][week2][start]">
                                            </div>
                                            <h5>à</h5>
                                            <div class="bfh-timepicker" data-mode="12h" data-time="14:00"
                                                 data-name="qouiqui[opening][week2][end]">
                                            </div>
                                        </div>

                                        <div class="col-md-6 nopadd">
                                            <div class="checkbox checkbox-primary">
                                                <input id="checkbox2" type="checkbox"
                                                       name="qouiqui[opening][weekendsamedi][open]" value="true">
                                                <label for="checkbox2">
                                                    Samedi de
                                                </label>
                                            </div>
                                            {#<input type="text" name="qouiqui[opening]" class="form-control"  value="{{ qouiqui.opening }}">#}

                                            <div class="bfh-timepicker" data-mode="12h" data-time="13:00"
                                                 data-name="qouiqui[opening][weekendsamedi][start]">
                                            </div>
                                            <h5>à</h5>
                                            <div class="bfh-timepicker" data-mode="12h" data-time="14:00"
                                                 data-name="qouiqui[opening][weekendsamedi][end]">
                                            </div>
                                        </div>

                                        <div class="col-md-6 nopadd">
                                            <div class="checkbox checkbox-primary">
                                                <input id="checkbox3" name="qouiqui[opening][weekenddimanche][open]"
                                                       value="true" type="checkbox">
                                                <label for="checkbox3">
                                                    Dimanche de
                                                </label>
                                            </div>
                                            {#<input type="text" name="qouiqui[opening]" class="form-control"  value="{{ qouiqui.opening }}">#}

                                            <div class="bfh-timepicker" data-mode="12h" data-time="13:00"
                                                 data-name="qouiqui[opening][weekenddimanche][start]">
                                            </div>
                                            <h5>à</h5>
                                            <div class="bfh-timepicker" data-mode="12h" data-time="14:00"
                                                 data-name="qouiqui[opening][weekenddimanche][end]">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <h4>Facebook </h4>
                                        <input type="text" name="qouiqui[rs][facebook]" class="form-control"
                                               value="">
                                    </div>
                                    <div class="col-md-12">
                                        <h4>Tweeter </h4>
                                        <input type="text" name="qouiqui[rs][tweeter]" class="form-control"
                                               value="">
                                    </div>
                                    <div class="col-md-12">
                                        <h4>Google Plus </h4>
                                        <input type="text" name="qouiqui[rs][google]" class="form-control"
                                               value="">
                                    </div>
                                    <div class="col-md-12">
                                        <h4>linkedin</h4>
                                        <input type="text" name="qouiqui[rs][linkedin]" class="form-control"
                                               value="">
                                    </div>
                                    {#{{ dump(encodeData(349)) }}#}

                                    <div class="col-md-12">
                                        <h4>Mots Clé</h4>
                                        {% if (encodeData(qouiqui)).tags is defined %}
                                            <input name="tags" type="text" class="form-control" id="tokenfield"
                                                   value="{{ (encodeData(qouiqui)).tags }}"/>
                                        {% else %}
                                            <input name="tags" type="text" class="form-control" id="tokenfield"
                                                   value=""/>
                                        {% endif %}
                                    </div>
                                </div>
                            </form>

                        </div>
                        <div role="tabpanel" class="tab-pane" id="browseTab">

                            <div class="centered row">
                                <div class="col-md-12">
                                    <img src="/uploads/image/{{ iduser }}/{{ qouiqui.id }}/avatar/avatar.jpg" alt=""
                                         alt="">
                                    <br>
                                    <form action="{{ path('module_spa_avatar',{ 'id': qouiqui.id }) }}" method="post"
                                          accept="image/gif, image/jpeg, image/png" enctype="multipart/form-data">
                                        <div class="col-md-7">

                                            <input type="file" name="fileToUpload" id="fileToUpload">
                                        </div>
                                        <div class="col-md-3">
                                            <input type="submit" class="btn btn-default " value="envoyer l'image"
                                                   class="btn-primary" name="submit">
                                        </div>
                                        <div class="col-md-2">


                                            <button type="button" class="btn btn-default delete_logo"
                                                    data-toggle="modal" data-target="#logoDelete"
                                            >supprimer

                                            </button>
                                        </div>
                                    </form>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Annuler</button>
            <button type="submit" class="btn btn-default " data-target="modifier" data-selector="edit_info">
            enregistrer
            </button>
            </div>


        </div>

    </div>
</div>
<div class="modal fade" id="logoDelete" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
     aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">


            <div class="modal-body">


                <input type="hidden" name="id_qq" value="{{ qouiqui.id }}" class="">

                <p>Voulez vous supprimer ce photo ?</p>
                <p class="debug-url"></p>

            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-dismiss="modal">Annuler</button>


                <button type="button" data-id="{{ qouiqui.id }}" class="btn btn-danger log_suprim ">
                    Supprimer
                </button>
            </div>
        </div>
    </div>
</div>

