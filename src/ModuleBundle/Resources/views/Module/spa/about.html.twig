<section class=" agent-section back_image " id="agent-section">

    <div class="auto-container">
        <div class="row clearfix">
            {#<a href="images/promo-1@2x.jpg" class="lightbox-single mfp-image">#}
            {#<img class="img-border" src="images/promo-1.jpg" data-at2x="images/promo-1@2x.jpg" alt>#}
            {#</a>#}
            <div class="column col-md-5 col-sm-12 col-xs-12 img-hiddens">
                <figure class="agent-image wow rollIn logo-resp" data-wow-delay="300ms" data-wow-duration="1500ms">
                    {#<img src="{{ asset('uploads/image/') }}/{{ iduser }}/{{ qouiqui.id }}/img/avatar.jpg" alt="">#}
                    {% set path =  asset('uploads/image/')~qouiqui.user.id ~'/'~ qouiqui.id ~'/avatar/avatar.jpg' %}
                    {% if asset_exists(path) %}
                        <img src="{{ asset('uploads/image/') }}{{ qouiqui.user.id }}/{{ qouiqui.id }}/avatar/avatar.jpg"
                             alt="">
                    {% else %}
                        <div  class="text_style">
                            <h1 style="  line-height: 70px;margin: 0px;font-size: 60px;font-family: 'Courgette', sans-serif;font-weight: 400; ">

                                Bienvenue chez  {{ qouiqui.firstName }}
                            </h1>
                        </div>
                    {% endif %}
                </figure>
            </div>
            {% set horaire = jsonDecod(qouiqui.opening) %}
            {% set sociaux = jsonDecod(qouiqui.sociaux) %}


            <div class="column col-md-7 col-sm-12 col-xs-12 text-align: center">


                <!--Bordered Title-->
                {#<div class="input-group input-group-sm">#}
                {#<input type="text" value="uuu" id="demo2"  class="form-control input-color"/>#}
                {#</div>#}

                <div class="titres  about">

                    <h2 class="section-title section-title-resp bottom-line font-alt grand-titre">A propos de nous</h2>

                    <div class="sep-lane-titre  sep-lane-titre-resp mt-50"><span><img
                                    src="{{ asset('module/spa/images/lines/img-line-0.png') }}"
                                    data-at2x="{{ asset('module/spa/images/lines/img-line-0@2x.png') }}"
                                    alt></span>
                    </div>
                </div>

                {#<h2>Welcome to <span class="theme_color">  Relish</span></h2>#}

                {#</div>#}


                <div class="desc-text about text">
                    <p>{{ qouiqui.details|raw }}
                    </p>
                </div>

                <!--Info Box-->
                <div class="info-box1">
                    <div class="col-md-6">
                        <ul class="clearfix">
                            <li class="address">
                                <div class="icon" {% if   theme.Couleur.fonticon is defined %} style="color: {{ theme.Couleur.fonticon }} !important;"{% endif %}>
                                    <span class="flaticon-location74"></span></div>
                                <h4 class="sous_titre1" {% if  theme.Couleur.sous_titre_sec1 is defined %} style="color: {{ theme.Couleur.sous_titre_sec1 }}" {% endif %} >
                                    ADDRESSE</h4>
                                <p class="text">{{ qouiqui.address1 }} <br>{{ qouiqui.adress2 }}</p>
                            </li>
                            <li class="contact-info">
                                <div class="icon" {% if   theme.Couleur.fonticon is defined %} style="color: {{ theme.Couleur.fonticon }} !important;"{% endif %}>
                                    <span class="flaticon-telephone51"></span></div>
                                <h4 class="sous_titre1" {% if theme.Couleur.sous_titre_sec1 is defined %} style="color: {{ theme.Couleur.sous_titre_sec1 }}" {% endif %}>
                                    TELEPHONE</h4>
                                <p class="text">{{ qouiqui.phone }}</p>
                            </li>
                            <li class="contact-info">
                                <div class="icon" {% if   theme.Couleur.fonticon is defined %} style="color: {{ theme.Couleur.fonticon }} !important;"{% endif %}>
                                    <i class="fa fa-clock-o" aria-hidden="true"></i></div>
                                <h4 class="sous_titre1" {% if  theme.Couleur.sous_titre_sec1 is defined %} style="color: {{ theme.Couleur.sous_titre_sec1 }}" {% endif %}>
                                    Horaire</h4>
                                {% if horaire != null %}
                                    <p class="text"> Ouvert de {{ horaire.week1.start }} à {{ horaire.week1.end }}<br>
                                        ouvert de {{ horaire.week2.start }} à {{ horaire.week2.end }}<br>

                                        {% if horaire.weekendsamedi.open is defined %}  Samedi du{{ horaire.weekendsamedi.start }} à {{ horaire.weekendsamedi.end }} {% else %}   Samedi est Fermé{% endif %}
                                        <br>
                                        {% if horaire.weekenddimanche.open is defined %}   Dimanche du {{ horaire.weekenddimanche.start }} à {{ horaire.weekenddimanche.end }}{% else %} Dimanche est Fermé {% endif %}
                                    </p>
                                {% endif %}
                            </li>
                        </ul>
                    </div>
                    <div class="col-md-6">
                        <ul class="clearfix">
                            <li class="address">
                                <div class="icon" {% if   theme.Couleur.fonticon is defined %} style="color: {{ theme.Couleur.fonticon }} !important;"{% endif %}>
                                    <span class="flaticon-configure"></span></div>
                                <h4 class="sous_titre1" {% if  theme.Couleur.sous_titre_sec1 is defined %} style="color: {{ theme.Couleur.sous_titre_sec1 }}" {% endif %}>
                                    Activité</h4>
                                <p class="text">{{ category.libelle }}</p>
                            </li>
                            <li class="contact-info">
                                <div class="icon" {% if   theme.Couleur.fonticon is defined %} style="color: {{ theme.Couleur.fonticon }} !important;"{% endif %}>
                                    <span class="flaticon-email2"></span></div>
                                <h4 class="sous_titre1" {% if  theme.Couleur.sous_titre_sec1 is defined %} style="color: {{ theme.Couleur.sous_titre_sec1 }}" {% endif %}>
                                    Email</h4>
                                <p class="text">{{ qouiqui.mail }}</p>
                            </li>
                            <li class="contact-info">
                                <div class="icon" {% if   theme.Couleur.fonticon is defined %} style="color: {{ theme.Couleur.fonticon }} !important;"{% endif %}>
                                    <span class="flaticon-house113"></span></div>
                                <h4 class="sous_titre1" {% if  theme.Couleur.sous_titre_sec1 is defined %} style="color: {{ theme.Couleur.sous_titre_sec1 }}" {% endif %}>
                                    SiteWeb</h4>
                                <a href="{{ qouiqui.website }}" class="text">{{ qouiqui.website }}</a>
                            </li>

                        </ul>
                    </div>
                    <div class="input-group input-group-sm">


                    </div>
                    <div class="col-md-6 pull-right">
                        <a href="#contact"
                           class="theme-btn btn-style-one ">Contactez Nous</a>
                        <a href="{% if qouiqui.geoLat and  qouiqui.geoLong %}http://maps.google.com?q={{ qouiqui.geoLat }},{{ qouiqui.geoLong }}{% else %}https://www.google.tn/maps/place/Tunisie/{% endif %}"
                           class="theme-btn btn-style-one">Y aller</a>

                        <div class="col-md-12 hidden-lg hidden-sm hidden-md pad-top imggoogle" style="padding-left:0">

                            <a target="_blanck"
                               href="{% if qouiqui.geoLat and  qouiqui.geoLong %}http://maps.google.com?q={{ qouiqui.geoLat }},{{ qouiqui.geoLong }}{% else %}https://www.google.tn/maps/place/Tunisie/{% endif %}">
                                <img src="https://maps.googleapis.com/maps/api/staticmap?zoom=17&size=600x300&maptype=roadmap
                                            &markers=color:blue%7C
                                            label:webmaster-media%7C{{ qouiqui.geoLat }},{{ qouiqui.geoLong }}
                                            &key=AIzaSyBTdMydc9PUE0gPkcgjRZXOuFPXc5BUSrg">
                            </a>


                        </div>


                    </div>

                    {#<div class="col-sm-6">#}
                    {#<div class="banner-soc align-left">#}
                    {#{% if  sociaux['facebook']   %}#}
                    {#<a href="{{ sociaux['facebook'] }}"><i class="flaticon-social"></i></a>#}
                    {#{% endif %} {% if  sociaux['tweeter']  %}#}
                    {#<a href="{{ sociaux['tweeter'] }}"><i class="flaticon-social"></i></a>#}
                    {#{% endif %} {% if  sociaux['google']   %}#}
                    {#<a href="{{  sociaux['google'] }}"><i class="flaticon-social"></i></a>#}
                    {#{% endif %}#}
                    {##}
                    {#</div>#}
                    {#</div>#}

                    <div class="col-sm-6  " style="top:20px;">
                        <div class="banner-soc align-left">
                            <a href="{{ sociaux['facebook'] }}"><i class="flaticon-social-4"></i></a>
                            <a href="{{ sociaux['tweeter'] }}"><i class="flaticon-social-5"></i></a>

                            <a href="{{ sociaux['linkedin'] }}"><i class="flaticon-social"></i></a>

                        </div>
                    </div>

                </div>

            </div>


        </div>
    </div>
