            </div>


            <div class="modal-body row">
                <div class="shop-cart padding-120">

                    <div class="shop-table table-responsive" >
                        <table>
                            <thead>
                            <tr>
                                <th colspan="2" class="title">Formation</th>
                                {#<th>Formateur</th>#}
                                <th class="start_cour">Date début</th>
                                <th class="end_cour">Date fin</th>
                                <th class="dure"> Durée</th>
                                {#<th class="descr">Description</th>#}
                                <th class="cart">Prix</th>

                                <th class="edit-cart">Modifier</th>
                                <th class="remove-cart">Supprimer</th>
                            </tr>
                            </thead>
                            <tbody>
                            {% for i, a in service %}
                                {% set services = jsonDecod(a.data) %}
                                <tr data-div={{ a.id }}>
                                    <td colspan="2" class="product-thumbnail">
                                        <div class="image">
                                            <a href=#"><img
                                                        src="{{ asset('uploads/image/') }}{{ iduser }}/{{ qouiqui.id }}/service/{{ a.id~'.jpg' }}"
                                                        style=" max-width: 40%;"
                                                        alt="product image"
                                                        class="img-responsive"></a>
                                        </div>
                                        <div class="content">
                                            <h4><a href="shop-page.html">{{ services.ncour }}</a></h4>
                                        </div>
                                    </td>
                                    {#<td><span>{{ services.trainer }}</span></td>#}
                                    <td class="start_cour"><span> {{ services.start|date('d-M-y') }}</span></td>
                                    <td class="end_cour"><span>{{ services.end|date('d-M-y') }}</span></td>
                                    <td class="dure"><span>{{ services.dure }}</span></td>
                                    {#<td class="descr">#}
                                    {#{{ services.desc[:9] }}...#}
                                    {#</td>#}
                                    <td class="cart"><span>{{ services.prix }}</span></td>
                                    <td class="edite-cart"><a href="#" class="modifier" data-serv="{{ a.id }}"
                                                              data-id="{{ qouiqui.id }}" data-toggle="modal"data-target="#edit_service">

                                            <span><i class="fa fa-pencil" aria-hidden="true"></i></span>
                                        </a>

                                    </td>
                                    <td class="remove-cart"><a href="#" class="service_suprim"
                                                               data-id='{{ a.id }}' data-toggle="modal"
