<div id="service-modal" class=" modal  fade" role="dialog">
    <div class="modal-dialog modal-lg  ">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <div class="pull-right margin-bott">

                    {#<a data-toggle="modal" href="#tallModal" class="btn btn-primary">Wide, Tall Content</a>#}
                    <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#modal_service"
                    >Ajouter Nouveau Cour
                    </button>
                </div>
