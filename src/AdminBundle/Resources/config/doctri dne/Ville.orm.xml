<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AdminBundle\Entity\Ville" table="VILLE">
    <indexes>
      <index name="fk_VILLE_PAYS1_idx" columns="PAYS_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="libelle" type="string" column="libelle" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="slug" type="string" column="slug" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="pays" target-entity="Pays" fetch="LAZY">
      <join-columns>
        <join-column name="PAYS_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
