<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AdminBundle\Entity\Immobiliair" table="IMMOBILIAIR">
    <unique-constraints>
      <unique-constraint name="id" columns="id"/>
    </unique-constraints>
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="adresse" type="string" column="adresse" length="50" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="geoLat" type="integer" column="geo_lat" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="geoLong" type="integer" column="geo_long" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="statu" type="integer" column="statu" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="cuisineEquipee" type="integer" column="cuisine_equipee" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="sallon" type="integer" column="sallon" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="suits" type="integer" column="suits" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="salleDeBain" type="integer" column="salle_de_bain" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="ascenseur" type="integer" column="ascenseur" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="archive" type="integer" column="archive" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
