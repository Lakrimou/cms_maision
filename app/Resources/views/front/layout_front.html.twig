    <script>


        function filtre() {
            v = jQuery('.lp-search-btn').val();
            info = jQuery('input[name="info"]').val();

            if (info == "") {

                link = '{{ path('user_homepage') }}recherche/' + v + '/all';
                location.replace(link);

            } else {
                link = '{{ path('user_homepage') }}recherche/' + v + '/' + info + '';
                location.replace(link);
            }
        }
    </script>
